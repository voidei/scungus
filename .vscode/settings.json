{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "yml",
    "toml",
    "markdown",
  ],
  "files.exclude": {
    "*.code-workspace": true,
    "*.lock": true,
    "*ignore*": false,
    ".*version": true,
    ".idea": true,
    ".yarn": false,
    "build": true,
    "node_modules": true,
    "packages": false,
    "types": true,
  },
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "*.lock": true,
    ".idea": true,
    ".yarn": true,
    "build": true,
    "packages": false,
  },
}
