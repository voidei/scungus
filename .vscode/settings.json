{
	"typescript.tsdk": "./node_modules/typescript/lib",
	"material-icon-theme.files.associations": {
		".markdownlint-cli2.jsonc": "settings",
		".markdownlint-cli2.yaml": "settings",
		".markdownlint-cli2.cjs": "settings",
		".markdownlint-cli2.mjs": "settings",
		".markdownlint.jsonc": "settings",
		".markdownlint.json": "settings",
		".markdownlint.yaml": "settings",
		".markdownlint.yml": "settings",
		".markdownlint.cjs": "settings",
		".markdownlint.mjs": "settings"
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"json",
		"jsonc",
		"json5",
		"yaml",
		"yml",
		"toml",
		"markdown"
	],
	"files.exclude": {
		"node_modules": true,
		//"node_modules": false,
		//"build": true,
		"build": false,
		".yarn": true,
		//".yarn": false,
		//"packages": true,
		"packages": false,
		"*.lock": true,
		//"*.lock": false,
		".idea": true,
		//".idea": false,
		//"types": true,
		"types": false
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/*.code-search": true,
		"build": true,
		".yarn": true,
		"packages": true,
		"*.lock": true,
		".idea": true
	},
	"files.associations": {
		"license": "plaintext"
	}
}
